{
  "name": "kawkah",
  "version": "0.1.0",
  "description": "CLI argument parser.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blujedis/kawkah.git"
  },
  "build": {
    "clean": [
      "./dist"
    ],
    "copy": [
      "./src/completions.sh.tpl|./dist/completions.sh.tpl"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "./build/spec"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "keywords": [
    "yargs",
    "commander",
    "parser",
    "minimist",
    "arguments",
    "cli"
  ],
  "author": "Blujedi <blujedicorp@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/blujedis/kawkah/issues"
  },
  "homepage": "https://github.com/blujedis/kawkah#readme",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/cross-spawn": "^6.0.0",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.5.2",
    "@types/read-pkg-up": "^3.0.1",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "mute-stream": "0.0.7",
    "nyc": "^11.9.0",
    "passpipe": "^1.0.1",
    "stiks": "^1.1.6",
    "ts-node": "^7.0.1",
    "tslint": "^5.10.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "argsert": "^1.0.9",
    "chek": "^1.3.1",
    "cross-spawn": "^6.0.5",
    "escape-string-regexp": "^1.0.5",
    "formatr": "^1.0.2",
    "js-yaml": "^3.12.0",
    "kawkah-parser": "github:blujedis/kawkah-parser",
    "lokales": "^1.2.3",
    "read-pkg-up": "^4.0.0",
    "tablur": "^1.0.8",
    "yargs": "^12.0.1"
  }
}